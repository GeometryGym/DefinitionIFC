<?xml version="1.0" encoding="utf-8"?>
<DocEntity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="IfcQuantityArea" UniqueId="3NuuyUK8L2Hv1ZScK4SB2G" DiagramNumber="1" BaseDefinition="IfcPhysicalSimpleQuantity" EntityFlags="229">
	<Attributes>
		<DocAttribute Name="AreaValue" UniqueId="1Bs3OpmIP0kwCW2sXflw9n" DefinedType="IfcAreaMeasure" AttributeFlags="0" AggregationType="0" AggregationFlag="0">
			<Documentation>Area measure value of this quantity.</Documentation>
		</DocAttribute>
	</Attributes>
	<WhereRules>
		<DocWhereRule Name="WR21" UniqueId="2rJPTlLUDBD9cHByB75qmM">
			<Documentation>If a unit is given, the unit type shall be area unit.</Documentation>
			<Expression>NOT(EXISTS(SELF\IfcPhysicalSimpleQuantity.Unit)) OR
   (SELF\IfcPhysicalSimpleQuantity.Unit.UnitType = IfcUnitEnum.AREAUNIT)</Expression>
		</DocWhereRule>
		<DocWhereRule Name="WR22" UniqueId="2VaVwQa2rBlhQHyzs8gwEV">
			<Documentation>A valid area quantity shall be greater than or equal to zero.</Documentation>
			<Expression>AreaValue &gt;= 0.</Expression>
		</DocWhereRule>
	</WhereRules>
</DocEntity>

