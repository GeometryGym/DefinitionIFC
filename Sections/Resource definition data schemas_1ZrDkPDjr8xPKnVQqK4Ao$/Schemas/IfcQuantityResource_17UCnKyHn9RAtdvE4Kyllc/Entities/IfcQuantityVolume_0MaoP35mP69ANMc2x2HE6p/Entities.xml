	<WhereRules>
		<DocWhereRule Name="WR21" UniqueId="3SZynh_w52wh7KiR5kGsDH">
			<Documentation>If a unit is given, the unit type shall be volume unit.</Documentation>
			<Expression>NOT(EXISTS(SELF\IfcPhysicalSimpleQuantity.Unit)) OR
