<?xml version="1.0" encoding="utf-8"?>
<DocEntity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="IfcQuantityLength" UniqueId="29lZg9FLXBtBGy3Kfq37$L" DiagramNumber="1" BaseDefinition="IfcPhysicalSimpleQuantity" EntityFlags="229">
	<Attributes>
		<DocAttribute Name="LengthValue" UniqueId="3u2KCNRAH6dRGZGaAGBLay" DefinedType="IfcLengthMeasure" AttributeFlags="0" AggregationType="0" AggregationFlag="0">
			<Documentation>Length measure value of this quantity.</Documentation>
		</DocAttribute>
	</Attributes>
	<WhereRules>
		<DocWhereRule Name="WR21" UniqueId="1P4zW8Yd55ghA1KCeYGyqS">
			<Documentation>If a unit is given, the unit type shall be a length unit.</Documentation>
			<Expression>NOT(EXISTS(SELF\IfcPhysicalSimpleQuantity.Unit)) OR
   (SELF\IfcPhysicalSimpleQuantity.Unit.UnitType = IfcUnitEnum.LENGTHUNIT)</Expression>
		</DocWhereRule>
		<DocWhereRule Name="WR22" UniqueId="3vbqOYcz96ZhDe9o7dvv1I">
			<Documentation>A valid length quantity shall be greater than or equal to zero.</Documentation>
			<Expression>LengthValue &gt;= 0.</Expression>
		</DocWhereRule>
	</WhereRules>
</DocEntity>

